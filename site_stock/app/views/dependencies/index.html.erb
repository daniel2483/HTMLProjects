<h1>Listing dependencies</h1>

<table>
  <tr>
    <th>Id sites</th>
    <th>Id dependencia</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dependencies.each do |dependency| %>
  <tr>
    <td><%= dependency.ID_SITES %></td>
    <td><%= dependency.ID_DEPENDENCIA %></td>
    <td><%= link_to 'Show', dependency %></td>
    <td><%= link_to 'Edit', edit_dependency_path(dependency) %></td>
    <td><%= link_to 'Destroy', dependency, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

