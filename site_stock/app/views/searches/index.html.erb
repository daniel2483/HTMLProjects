<h1>Listing searches</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th>Nombre</th>
    <th>Isla</th>
    <th>Medio</th>
    <th>Isla</th>
    <th>Tipo</th>
    <th>Ip</th>
    <th>New</th>
    <th>Show</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @searches.each do |search| %>
  <tr>
    <td><%= search.CODiGO %></td>
    <td><%= search.NOMBRE %></td>
    <td><%= search.ISLA %></td>
    <td><%= search.MEDIO %></td>
    <td><%= search.ISLA %></td>
    <td><%= search.TIPO %></td>
    <td><%= search.IP %></td>
    <td><%= search.new %></td>
    <td><%= search.show %></td>
    <td><%= link_to 'Show', search %></td>
    <td><%= link_to 'Edit', edit_search_path(search) %></td>
    <td><%= link_to 'Destroy', search, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

